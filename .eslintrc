{
  "extends": ["airbnb","prettier"],
  "rules": {
    "no-console": 0,
    "arrow-body-style": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "no-restricted-syntax": 0,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "object-shorthand": 0,
    "class-methods-use-this": 0
  }
}
